        "class AFNComprimentoPar:\n",
        "    def aceita(self, string):\n",
        "        return len(string) % 2 == 0\n",
        "\n",
        "afd9 = AFNComprimentoPar()\n",
        "\n",
        "print(afd9.aceita(\"abab\"))  # True\n",
        "print(afd9.aceita(\"abc\"))   # False\n"
